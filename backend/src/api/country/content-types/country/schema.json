{
    "kind": "collectionType",
    "collectionName": "countries",
    "info": {
        "singularName": "country",
        "pluralName": "countries",
        "displayName": "Country",
        "name": "country"
    },
    "options": {
        "draftAndPublish": false
    },
    "attributes": {
        "name": {
            "type": "string",
            "required": true,
            "unique": true
        },
        "code": {
            "type": "string",
            "required": true,
            "unique": true,
            "maxLength": 2
        },
        "phoneCode": {
            "type": "string",
            "required": true
        },
        "currency": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::currency.currency"
        },
        "isActive": {
            "type": "boolean",
            "default": true
        }
    },
    "pluginOptions": {
        "content-manager": {
            "visible": true
        },
        "content-type-builder": {
            "visible": true
        }
    }
}
