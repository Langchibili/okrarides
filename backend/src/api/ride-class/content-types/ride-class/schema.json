{
  "kind": "collectionType",
  "collectionName": "ride_classes",
  "info": {
    "singularName": "ride-class",
    "pluralName": "ride-classes",
    "displayName": "Ride Class",
    "name": "rideClass"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true
    },
    "content-type-builder": {
      "visible": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "taxiType": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::taxi-type.taxi-type"
    },
    "baseFare": {
      "type": "decimal",
      "required": true
    },
    "perKmRate": {
      "type": "decimal",
      "required": true
    },
    "perMinuteRate": {
      "type": "decimal",
      "required": true
    },
    "minimumFare": {
      "type": "decimal",
      "required": true
    },
    "commissionPercentage": {
      "type": "decimal"
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "displayOrder": {
      "type": "integer",
      "default": 0
    },
    "vehicles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::vehicle.vehicle",
      "inversedBy": "rideClasses"
    }
  }
}
